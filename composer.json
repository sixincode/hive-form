{
    "name": "sixincode/hive-form",
    "description": "Package for Form and Icon elements",
    "keywords": [
        "sixincode",
        "laravel",
        "hive-form"
    ],
    "homepage": "https://github.com/sixincode/hive-form",
    "license": "MIT",
    "authors": [
        {
            "name": "Sixincode",
            "email": "sixincode@6ixin.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "sixincode/modules-init": "^1.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Sixincode\\HiveForm\\": "src",
            "Sixincode\\HiveForm\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sixincode\\HiveForm\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sixincode\\HiveForm\\HiveFormServiceProvider"
            ],
            "aliases": {
                "HiveForm": "Sixincode\\HiveForm\\Facades\\HiveForm"
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/sixincode/"
        },
        {
            "packagist.org": false
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
